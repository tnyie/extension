{
  "manifest_version": 2,
  "name": "TnyIE URL Shortener",
  "homepage_url": "https://ui.tny.ie",
  "description": "Extension to shorten urls",
  "default_locale": "en",
  "permissions": [
    "tabs"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "TnyIE",
    "default_icon": {
      "128": "icons/128.png"
    }
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  }
}
